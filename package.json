{
	"name": "donde-esta-tu-local-v5",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "prisma generate --no-engine && prisma migrate deploy && bun run prisma migrate deploy && vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"paraglide-js": "paraglide-js compile --project ./project.inlang --outdir ./src/lib/paraglide",
		"db:seed": "tsx prisma/seed.ts"
	},
	"devDependencies": {
		"@eslint/compat": "^1.3.2",
		"@eslint/js": "^9.33.0",
		"@inlang/paraglide-js": "^2.2.0",
		"@lucide/svelte": "^0.539.0",
		"@sveltejs/adapter-auto": "^6.1.0",
		"@sveltejs/kit": "^2.35.0",
		"@sveltejs/vite-plugin-svelte": "^6.1.3",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.16",
		"@tailwindcss/vite": "^4.1.12",
		"@types/leaflet": "^1.9.20",
		"@types/node": "^24.3.0",
		"daisyui": "^5.0.50",
		"eslint": "^9.33.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.11.0",
		"formsnap": "^2.0.1",
		"globals": "^16.3.0",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"svelte": "^5.38.2",
		"svelte-check": "^4.3.1",
		"sveltekit-superforms": "^2.27.1",
		"tailwindcss": "^4.1.12",
		"tsx": "^4.20.4",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.40.0",
		"vite": "^7.1.3"
	},
	"dependencies": {
		"@prisma/client": "^6.14.0",
		"@prisma/extension-accelerate": "^2.0.2",
		"@vercel/blob": "^1.1.1",
		"better-auth": "^1.3.7",
		"leaflet": "^1.9.4",
		"pino": "^9.9.0",
		"pino-pretty": "^13.1.1",
		"prisma": "^6.14.0"
	}
